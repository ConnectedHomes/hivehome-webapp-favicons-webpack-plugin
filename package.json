{
  "name": "@connected-home/hivehome-webapp-favicons-webpack-plugin",
  "version": "1.1.8",
  "description": "Favicon Generator for Hivehome Webapp",
  "main": "lib/index.js",
  "engines": {
    "node": ">=6.0.0"
  },
  "files": [
    "lib/"
  ],
  "scripts": {
    "lint": "eslint --cache src/",
    "compile": "babel -d lib/ src/ ",
    "clean": "rm -rf lib/*",
    "precommit": "npm run compile",
    "fmt": "prettier-config --write \"src/**/*.js\"",
    "fmt:check": "prettier-config --list-different \"src/**/*.js\"",
    "fmt:glob": "prettier-config --write"
  },
  "standard": {
    "ignore": [
      "node_modules/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ConnectedHomes/hivehome-webapp-favicons-webpack-plugin.git"
  },
  "keywords": [
    "webpack",
    "plugin",
    "html-webpack-plugin",
    "favicon",
    "icon"
  ],
  "author": "Peter Bevan <peter.bevan@hivehome.com>",
  "private": false,
  "bugs": {
    "url": "https://github.com/ConnectedHomes/hivehome-webapp-favicons-webpack-plugin/issues"
  },
  "licence": "MIT",
  "homepage": "https://github.com/ConnectedHomes/hivehome-webapp-favicons-webpack-plugin#readme",
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-stage-0": "^6.16.0",
    "babel-root-import": "^4.1.3",
    "eslint": "^3.19.0",
    "eslint-config-prettier": "^1.6.0",
    "json-loader": "^0.5.4",
    "pre-commit": "^1.1.3",
    "prettier": "^1.0.0",
    "prettier-config": "^1.0.0"
  },
  "peerDependencies": {
    "webpack": ">= 1.14.x <= 3.x"
  },
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "hasha": "^3.0.0",
    "jimp": "^0.2.28",
    "tinycolor2": "^1.4.1",
    "to-ico": "^1.1.4"
  }
}
