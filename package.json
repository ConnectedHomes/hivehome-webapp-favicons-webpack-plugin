{
  "name": "hivehome-webapp-favicons-webpack-plugin",
  "version": "1.0.0",
  "description": "Favicon Generator for Hivehome Webapp",
  "main": "lib/index.js",
  "engines": {
    "node": ">=6.0.0"
  },
  "files": [
    "lib/"
  ],
  "scripts": {
    "lint": "eslint --cache src/",
    "compile": "babel --presets es2015,stage-0 -d lib/ src/ ",
    "clean": "rm -rf lib/*",
    "precommit": "npm run compile",
    "fmt": "prettier-config --write \"src/**/*.js\"",
    "fmt:check": "prettier-config --list-different \"src/**/*.js\"",
    "fmt:glob": "prettier-config --write"
  },
  "standard": {
    "ignore": [
      "node_modules/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ConnectedHomes/hivehome-webapp-favicons-webpack-plugin.git"
  },
  "keywords": [],
  "author": "Peter Bevan <peter.bevan@bgch.co.uk>",
  "private": true,
  "bugs": {
    "url": "https://github.com/ConnectedHomes/hivehome-webapp-favicons-webpack-plugin/issues"
  },
  "homepage": "https://github.com/ConnectedHomes/hivehome-webapp-favicons-webpack-plugin#readme",
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-async-functions": "^6.8.0",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-async-to-module-method": "^6.16.0",
    "babel-plugin-transform-regenerator": "^6.16.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-root-import": "^4.1.3",
    "babel-runtime": "^6.11.6",
    "eslint": "^3.19.0",
    "eslint-config-prettier": "^1.6.0",
    "json-loader": "^0.5.4",
    "md5-file": "^3.1.1",
    "pre-commit": "^1.1.3",
    "prettier": "^1.0.0",
    "prettier-config": "^1.0.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.16.0",
    "jimp": "^0.2.28",
    "to-ico": "^1.1.4",
    "webpack": "1.14.0"
  }
}
